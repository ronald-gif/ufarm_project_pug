                | Urban Farmer
              button.btn-close.mb-4.pb-4.btn-close-black(type='button' data-bs-dismiss='modal' aria-label='Close')
            .modal-body(style='color: #000;')
              // sign up for farmer one
              form#myFormToRegister(action='#' name='registration')
                .container.thisForm
                  .row.mb-4
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') First name:
                        input#firstName.firstName(type='text')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') Second name:
                        input#secondName.secondName(type='text')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') Phone number:
                        input#telephone.telephone(type='text')
                        small.error
                  .row.mb-4
                    .col
                      .form-outline
                        label.form-label(for='currentDate')  Current date:
                        input#currentDate.currentDate(type='date')
                    .col
                      .form-outline
                        label.form-label(for='exampleInputEmail1') Date of birth:
                        input#DOB.DOB(type='date')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') Unique number:
                        input#uniqueNumber.uniqueNumber(type='text')
                        small.error
                  .row.mb-4
                    .col
                      .form-outline
                        label.form-label(for='exampleInputPassword1') NIN Number:
                        input#NIN.NIN(type='text')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') Location:
                        input#location.location(type='text')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') Residence type:
                        input#residence.residence(type='text')
                        small.error
                  .row.mb-4
                    .col
                      .form-field.radio
                        label(for='gender') Gender:
                        .radio
                          input#gender1(type='radio' name='sex' checked='')
                          span Male
                          input#gender2(type='radio' name='sex')
                          span Female
                    .col-4
                      .form-o
                        label(for='ward') Ward
                        select#my_ward.values-for-wards(name='ward')
                          option(value='ward1') LC1
                          option(value='ward2') LC2
                          option(value='ward3') LC3
                          option(value='ward4') LC4
                  .row.mb-4
                    .col
                      .form-field.radio
                        label(for='lang') Activities:
                        .radio
                          input#mobile(type='checkbox' name='Luganda' value='language')
                          span Mobilization
                          input#supervise(type='checkbox' name='english' value='language')
                          span Supervision
                          input#regulate(type='checkbox' name='english' value='language')
                          span Regulation
                          input#support(type='checkbox' name='english' value='language')
                          span Support
                          small.error
                  .row.mb-4
                    .col
                      .mb-3.form-check
                        input#exampleCheck1.form-check-input(type='checkbox')
                        label.form-check-label(for='exampleCheck1') Comfirm
                      button.btn(type='submit') Submit
