                        input#telephone.telephone(type='text')
                        small.error
                  .row.mb-4
                    .col
                      .form-outline
                        label.form-label(for='exampleInputPassword1')  Current date:
                        input#exampleInputPassword1.form-control(type='date')
                    .col
                      .form-outline
                        label.form-label(for='exampleInputEmail1') Date of birth:
                        input#DOB.DOM(type='date')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') ID:
                        input#uniqueNumber.uniqueNumber(type='text')
                        small.error
                  .row.mb-4
                    .col
                      .form-outline
                        label.form-label(for='exampleInputPassword1') NIN Number:
                        input#NIN.NIN(type='nin')
                        small.error
                    .col
                      .form-outline
                        label.form-label(for='form3Example1') Location/Division:
                        input#location.location(type='text')
                        small.error
                    .col
                      //
                        <div class="form-outline">
                        <label class="form-label" for="form3Example1">Residence type:</label>
                        <input type="text" id="form3Example1" class="form-control" />
                        </div>
                  .row.mb-4
                    .col
                      .form-field.radio
                        label(for='gender') Gender:
                        .radio
                          input#gender1(type='radio' name='sex' checked='')
                          span Male
                          input#gender2(type='radio' name='sex')
                          span Female
                    .col-4
                      .form-o
                        label(for='ward') Ward
                        select#my_ward.values-for-wards(name='ward')
                          option(value='ward1') LC1
                          option(value='ward2') LC2
                          option(value='ward3') LC3
                          option(value='ward4') LC4
                  .row.mb-4
                    .col
                      .form-field.radio
                        label(for='lang') Activities:
                        .radio
                          input#harticulture(type='checkbox' name='Luganda' value='language')
                          span Horticulture produce
                          input#poultry(type='checkbox' name='english' value='language')
                          span Poultry produce
                          input#dairy(type='checkbox' name='english' value='language')
                          span Dairy produce
                          small.error
                  .row.mb-4
                    .col
                      .mb-3.form-check
                        input#exampleCheck1.form-check-input(type='checkbox')
                        label.form-check-label(for='exampleCheck1') Comfirm
                      button.btn(type='submit') Submit
